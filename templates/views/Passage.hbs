      markes.innerHTML = `Marked For Review ${m}`;

      inner.style.display = 'initial';
      questioncount = 0;
      loadquestion();
      let timer = 5* 60;
      let i = null;
      let p = document.querySelector('#timer');
      let outs = document.querySelector('#out');
      function updatetime() {
        if (timer > -1) {

          let minute = Math.floor(timer / 60);
          let second = timer % 60;
          p.innerText = `Time Remaining is : ${minute}minutes:${second}seconds`;
          timer--;

        } else {
          let k=0;
          const wrongansarray=[];
          inner.style.display = 'none';
          clearInterval(i);
          outs.innerText = "Time out";
          for (let i = 0; i < quiz.length; i++) {
            if (quiz[i].stuAns === quiz[i].ans) {
              score++;
            }
            else if(quiz[i].stuAns!==null){
              score=score-0.25;
               wrongansarray[k++]=i+1;
            }
           
          }
           Wrongans.innerHTML = `Wrong Answers Are ::  ${wrongansarray}<br> `;
          show.innerHTML = `You scored ::  ${score}/${quiz.length}<br> 
      <button class="btn bg-primary mt-3 text-white" onclick="location.reload()">Resume</button> `;


        }
      }
      i = setInterval(updatetime, 1000);
    }
  </script>
</body>

</html>